												  <i class="fa fa-star"></i>
												  <i class="fa fa-star"></i>
												  <i class="fa fa-star"></i>
												  <i class="fa fa-star-o"></i>
												</div>
										  
												<div class="product-btns">
												  
												  <a href="/auction/<%= a.id %>" class="quick-view"><i class="fa fa-eye"></i><span class="tooltipp">Quick view</span></a>
												</div>
											  </div>
										  
											  <div class="add-to-cart">
												<a href="/auction/<%= a.id %>" class="add-to-cart-btn"><i class="fa fa-gavel"></i> View Auction</a>
											  </div>
											</div>
