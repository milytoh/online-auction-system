											<div class="product">
											  <div class="product-img">
												<img src="/images/<%= a.image %>" alt="<%= a.title %>">
												<div class="product-label">
												  <% if (a.status === 'active') { %>
													<span class="new">ACTIVE</span>
												  <% } else { %>
													<span class="new" style="background: #D10024;">CLOSED</span>
												  <% } %>
